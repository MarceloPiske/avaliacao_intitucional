{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/institucional/**",
        "destination": "/avaliacao_institucional/index.html"
      },
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/disciplinas",
        "destination": "/index.html"
      }
    ]
  }
}
